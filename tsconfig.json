{
  "compilerOptions": {
    "allowJs": true,
    // "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    // "composite": true,
    // "declaration": true,
    // "declarationMap": true,
    "esModuleInterop": true,
    // "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "moduleResolution": "Node",
    // "noEmit": true,
    // "noUnusedLocals": false,
    // "noUnusedParameters": true,
    "outDir": "build",
    "rootDir": "src",
    // "sourceMap": true,
    "strict": true,
    "target": "es6",
    "resolveJsonModule": true,
    "skipLibCheck": true,
  },
  "target": "es5",
  "include": [
    "src/**/*",
    // "jest.config.unit.js"
  ],
}